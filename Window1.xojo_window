#tag EndWindow

#tag WindowCode
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  reset_window
		  return true
		End Function
	#tag EndEvent

	#tag Event
		Sub Open()
		  scaleFactor = self.Width / 550
		  reset_window
		  
		End Sub
	#tag EndEvent

	#tag Event
		Sub Resized()
		  self.Height = Round(self.Width*2/5)
		  scaleFactor = self.Width / 550
		  
		  For i As Integer = 0 To Self.ControlCount-1
		    If Self.Control(i) IsA Custom_label Then
		      custom_label(control(i)).scale
		    End If
		  Next
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub clear_5min_display()
		  min_half.clear
		  min_ten.clear
		  min_quarter.clear
		  min_twenty.clear
		  min_five.clear
		  min_minutes.clear
		  min_to.clear
		  min_past.clear
		  min_oclock.clear
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub clear_hour_display()
		  hour_one.clear
		  hour_two.clear
		  hour_three.clear
		  hour_four.clear
		  hour_five.clear
		  hour_six.clear
		  hour_seven.clear
		  hour_eight.clear
		  hour_nine.clear
		  hour_ten.clear
		  hour_eleven.clear
		  hour_twelve.clear
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Function get_5min(d as Date) As integer
		  return floor(val(Mid(d.shorttime,instr(d.ShortTime,":")+1,2))/5)*5
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function get_hour(d as Date) As integer
		  return val(left(d.ShortTime,InStr(d.ShortTime,":")-1))
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub reset_window()
		  set_random_colours
		  display_it_is.set
		  
		  dim d as new Date
		  
		  time_hour = get_hour(d)
		  time_5min = get_5min(d)
		  set_hour_display
		  set_5min_display
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
