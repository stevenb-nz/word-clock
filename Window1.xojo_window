#tag Events timeTimer
	#tag Event
		Sub Action()
		  dim d as new Date
		  dim d_5min, d_hour as integer
		  
		  d_5min = get_5min(d)
		  if d_5min <> time_5min then
		    time_5min = d_5min
		    set_5min_display
		  end if
		  d_hour = get_hour(d)
		  if d_hour <> time_hour then
		    time_hour = d_hour
		    set_hour_display
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
